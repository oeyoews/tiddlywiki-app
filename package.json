    "electron": "^37.3.0",
    "electron-builder": "^26.0.12",
    "playwright": "^1.54.2",
    "tw5-typed": "^0.5.14",
    "vite": "^6.3.5",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "optionalDependencies": {
    "sharp": "^0.33.5",
    "svgo": "^3.3.2"
  },
  "engines": {
    "node": ">= 18"
  },
  "volta": {
    "node": "22.17.0"
  }
